<div>
<a href="/getdetails">View Results</a>
<a href="/form">Fill Out Survey</a>
</div>
<hr>
    
    <% if(details!=null) { %>
    <table>
    <tr>
        <th>Name</th>
        <th>Email </th>
        <th>Phone Number</th>
        <th>Level of Interest </th>
        <th>Visa Needed </th>
        <th>Visa</th>
        <th>Languages</th>
    </tr>
    <% details.forEach(function(item){ %>
    <tr>
        <td><a href="/delete/<%= item._id%>" class="delete"><%= item.name%></a></td>
        <td><%= item.email %></td>
        <td><%= item.phoneNumber%></td>
        <td><%= item.interestLevel %></td>
        <td><%= item.visaNeeded%></td>
        <td><%= item.visa%></td>
        <td><%= item.codingLanguages%></td>
    
    </tr>
    <% }) %>
    </table>
    <script>
        const elementToDelete = document.getElementsByClassName('delete');
        function warnThenDelete(e) {
            if (!confirm('You are permenatly deleting this result from the surver, are you sure you wish to proceed?')) e.preventDefault();
        };
        for (var i = 0, l = elementToDelete.length; i < l; i++) {
            elementToDelete[i].addEventListener('click', warnThenDelete, false);
        }
    </script>
    <% } %>